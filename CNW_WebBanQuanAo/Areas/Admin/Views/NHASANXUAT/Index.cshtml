@model IEnumerable<CNW_WebBanQuanAo.Models.NHASANXUAT>

@{
    ViewBag.Title = "Index";
}

<h2>Danh sách nhà sản xuất</h2>

<p>
    @Html.ActionLink("Thêm nhà sản xuất", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenNSX)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiaChi)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenNSX)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiaChi)
        </td>
        <td>
            @Html.ActionLink("Chỉnh sửa", "Edit", new { id=item.MaNSX }) |
            @Html.ActionLink("Chi tiết", "Details", new { id=item.MaNSX }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.MaNSX })
        </td>
    </tr>
}

</table>
